<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/text_color_white"
    android:orientation="vertical">

    <ScrollView
        android:id="@+id/user_info_scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/user_info_bottom_layout"
        android:layout_alignParentTop="true"
        android:fillViewport="true"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/user_info_head_img_layout"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:background="@color/text_color_black"
                android:visibility="visible">

                <ImageView
                    android:id="@+id/user_info_head_img_iv"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <LinearLayout
                    android:id="@+id/user_info_head_img_bottom_hint"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/user_info_video_state_iv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginBottom="8dp"
                        android:layout_marginRight="12dp"
                        android:background="@drawable/user_info_video_invite_icon"
                        android:visibility="visible" />

                    <RelativeLayout
                        android:id="@+id/user_info_hint_tv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:background="@drawable/user_info_hint_bg"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableLeft="@drawable/hint_icon"
                            android:drawablePadding="7dp"
                            android:gravity="center_vertical"
                            android:paddingBottom="5dp"
                            android:paddingLeft="15dp"
                            android:paddingTop="5dp"
                            android:text="尚未通过头像认证"
                            android:visibility="visible" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:src="@drawable/headimg_no_pass_left_icon" />
                    </RelativeLayout>
                </LinearLayout>

                <ImageView
                    android:id="@+id/user_info_faceRating_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_gravity="right"
                    android:layout_marginBottom="15dp"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/user_info_facerating_icon"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/user_info_faceRating_chou_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="-100dp"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/user_info_facerating_chou_icon"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/user_info_faceRating_yiban_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="-100dp"
                    android:layout_marginLeft="13dp"
                    android:layout_toRightOf="@+id/user_info_faceRating_chou_iv"
                    android:background="@drawable/user_info_facerating_yiban_icon"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/user_info_faceRating_shuai_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="-100dp"
                    android:layout_marginLeft="13dp"
                    android:layout_toRightOf="@+id/user_info_faceRating_yiban_iv"
                    android:background="@drawable/user_info_facerating_shuai_icon"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/user_info_faceRating_henshuai_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="-100dp"
                    android:layout_marginLeft="13dp"
                    android:layout_toRightOf="@+id/user_info_faceRating_shuai_iv"
                    android:background="@drawable/user_info_facerating_henshuai_icon"
                    android:visibility="gone" />

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="3dp">

                        <TextView
                            android:id="@+id/user_info_detail_name_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="5dp"
                            android:paddingLeft="2dp"
                            android:singleLine="true"
                            android:ellipsize="end"
                            android:maxEms="6"
                            android:text="姓名"
                            android:textColor="@color/text_color_424242"
                            android:textSize="14sp" />

                        <ImageView
                            android:id="@+id/user_info_face_lable_iv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="5dp"
                            android:layout_toRightOf="@+id/user_info_detail_name_tv"
                            android:src="@drawable/high_quality_icon"
                            android:visibility="visible" />


                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/user_info_sex_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="5dp"
                            android:paddingLeft="2dp"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="@color/text_color_white"
                            android:textSize="10sp" />

                        <TextView
                            android:id="@+id/user_info_time_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:singleLine="true"
                            android:text="时间"
                            android:textColor="@color/text_color_424242"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/user_info_time_distance_line"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:singleLine="true"
                            android:text="|"
                            android:textColor="@color/text_color_424242"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/user_info_distance_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:singleLine="true"
                            android:text="距离"
                            android:textColor="@color/text_color_424242"
                            android:textSize="14sp" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="6dp"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/user_info_hor_iv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:background="@drawable/user_info_hor_baiyang" />

                    <TextView
                        android:id="@+id/user_info_hor_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="2dp"
                        android:singleLine="true"
                        android:text=""
                        android:textColor="@color/text_color_b2b2b2"
                        android:textSize="10sp" />


                </LinearLayout>

                <View
                    android:id="@+id/user_info_match_line_view"
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:visibility="gone"
                    android:background="@color/text_color_e7e7e7" />

                <LinearLayout
                    android:id="@+id/user_info_everyday_match_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="6dp"
                    android:visibility="gone"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/user_info_everyday_match_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="2dp"
                        android:singleLine="true"
                        android:text="星座匹配"
                        android:textColor="@color/text_color_333333"
                        android:textSize="12sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="4dp"
                        android:layout_centerVertical="true"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/user_info_everyday_match_one_star"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/score_star" />

                        <ImageView
                            android:id="@+id/user_info_everyday_match_two_star"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="3dp"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/score_star" />

                        <ImageView
                            android:id="@+id/user_info_everyday_match_three_star"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="3dp"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/score_star" />

                        <ImageView
                            android:id="@+id/user_info_everyday_match_four_star"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="3dp"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/score_star" />

                        <ImageView
                            android:id="@+id/user_info_everyday_match_five_star"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="3dp"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/score_star" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


            <cn.chono.yopper.view.MyGridView
                android:id="@+id/user_info_album_gridview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="0dp"
                android:gravity="center_horizontal"
                android:horizontalSpacing="4dp"
                android:listSelector="@android:color/transparent"
                android:numColumns="4"
                android:scrollbars="none"
                android:verticalSpacing="4dp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/text_color_dcdcdc" />

            <LinearLayout
                android:id="@+id/user_info_appoint_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            </LinearLayout>

            <View
                android:id="@+id/user_dating_line"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/text_color_dcdcdc" />


            <RelativeLayout
                android:id="@+id/user_info_bubble_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingBottom="10dp"
                android:paddingTop="10dp">

                <LinearLayout
                    android:id="@+id/user_info_bubble_tv_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="冒泡"
                        android:textColor="@color/text_color_939393"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/user_info_bubble_num_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:gravity="center"
                        android:singleLine="true"
                        android:text="0"
                        android:textColor="@color/text_color_58668d"
                        android:textSize="20sp" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/user_info_bubble_img_layout"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@+id/user_info_bubble_tv_layout">

                    <ImageView
                        android:id="@+id/user_info_bubble_img_iv"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="center" />

                    <ImageView
                        android:id="@+id/user_album_border"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:background="@drawable/img_circle_bg"
                        android:visibility="visible" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:layout_toLeftOf="@+id/user_info_bubble_left_iv"
                    android:layout_toRightOf="@+id/user_info_bubble_img_layout"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/user_info_bubble_title_tv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text=""
                        android:textColor="@color/text_color_black"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/user_info_bubble_content_tv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text=""
                        android:textColor="@color/text_color_939393"
                        android:textSize="14sp" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/user_info_bubble_left_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:src="@drawable/user_info_arrow_let" />
            </RelativeLayout>

            <View
                android:id="@+id/user_bubble_line"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/text_color_dcdcdc" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:singleLine="true"
                    android:text="交友资料"
                    android:textColor="@color/text_color_9e9e9e"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/user_info_isprofilecomplete_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="(未完善)"
                    android:textColor="@color/text_color_ff7462"
                    android:textSize="14sp"
                    android:visibility="gone" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="8dp"
                android:paddingTop="8dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/user_info_age_icon" />

                <TextView
                    android:layout_width="56sp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:text="年龄"
                    android:textColor="@color/text_color_9e9e9e"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/user_info_age_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:text="100"
                    android:textColor="@color/text_color_black"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/user_info_age_level_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/age_level_bg"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:singleLine="true"
                    android:text="90后"
                    android:textColor="@color/text_color_white"
                    android:textSize="10sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="8dp"
                android:paddingTop="8dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/user_info_emotional_icon" />

                <TextView
                    android:layout_width="56sp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:text="情感状态"
                    android:textColor="@color/text_color_9e9e9e"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/user_info_emotional_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:textColor="@color/text_color_black" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/user_info_height_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="8dp"
                android:paddingTop="8dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/user_info_height_icon" />

                <TextView
                    android:layout_width="56sp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:text="身高"
                    android:textColor="@color/text_color_9e9e9e"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/user_info_height_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/text_color_black"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/user_info_weight_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="8dp"
                android:paddingTop="8dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/user_info_weight_icon" />

                <TextView
                    android:layout_width="56sp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:text="体重"
                    android:textColor="@color/text_color_9e9e9e"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/user_info_weight_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/text_color_black"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="8dp"
                android:paddingTop="8dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/user_info_profession_icon" />

                <TextView
                    android:layout_width="56sp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:text="职业"
                    android:textColor="@color/text_color_9e9e9e"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/user_info_profession_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/text_color_black"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="8dp"
                android:paddingTop="8dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/user_info_income_icon" />

                <TextView
                    android:layout_width="56sp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:text="收入"
                    android:textColor="@color/text_color_9e9e9e"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/user_info_income_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/text_color_black"
                    android:textSize="14sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/text_color_dcdcdc" />

            <LinearLayout
                android:id="@+id/user_info_lable_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:singleLine="true"
                    android:text="标签"
                    android:textColor="@color/text_color_9e9e9e"
                    android:textSize="14sp" />

                <cn.chono.yopper.view.FlowLeftLayout
                    android:id="@+id/user_info_lable_flowlayout"
                    android:layout_width="match_parent"
