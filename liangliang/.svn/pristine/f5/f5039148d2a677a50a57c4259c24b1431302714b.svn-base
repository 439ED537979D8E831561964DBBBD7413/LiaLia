<?xml version="1.0" encoding="UTF-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:shareInterpolator="false" >

    <!-- 旋转 -->

    <rotate
        android:duration="530"
        android:fromDegrees="46"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toDegrees="0" />

    <!-- 移动 -->

    <translate
        android:duration="530"
        android:fromXDelta="362"
        android:fromYDelta="229"
        android:toXDelta="0"
        android:toYDelta="0" />

    <!-- 放大 -->

    <scale
        android:duration="530"
        android:fromXScale="0.8"
        android:fromYScale="0.8"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toXScale="1.0"
        android:toYScale="1.0" />

</set>