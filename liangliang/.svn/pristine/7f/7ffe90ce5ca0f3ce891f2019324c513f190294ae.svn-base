<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/background_gray_white">

    <FrameLayout
        android:id="@+id/frame_content"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/bottom_tab_container_layout"
        android:gravity="center"
        android:orientation="vertical"></FrameLayout>

    <LinearLayout
        android:id="@+id/bottom_tab_container_layout"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/index_bottom_layout_bg"
        android:orientation="horizontal"
        android:visibility="visible">

        <LinearLayout
            android:id="@+id/nearby_layout_indexActivity"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/icon_nearby_narmal_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_nearby_click" />

            <TextView
                android:id="@+id/icon_nearby_narmal_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/nearby_str"
                android:textColor="@color/text_color_tab_style"
                android:textSize="10sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/appointment_layout_indexActivity"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/icon_appointment_layout_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_appointment_click" />

            <TextView
                android:id="@+id/icon_appointment_narmal_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/appointment_str"
                android:textColor="@color/text_color_tab_style"
                android:textSize="10sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/find_layout_indexActivity"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/icon_find_narmal_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_find_click" />

            <TextView
                android:id="@+id/icon_find_narmal_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/find_str"
                android:textColor="@color/text_color_tab_style"
                android:textSize="10sp" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/message_layout_indexActivity"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/icon_message_narmal_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/icon_message_click" />


                <TextView
                    android:id="@+id/index_message_bubble_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="-2dp"
                    android:layout_toRightOf="@+id/icon_message_narmal_icon"
                    android:background="@drawable/circle_messaga_num_bg"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="0"
                    android:textColor="@color/text_color_white"
                    android:textSize="10sp"
                    android:visibility="visible" />


            </RelativeLayout>


            <TextView
                android:id="@+id/icon_message_narmal_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/icon_message_narmal_icon"
                android:layout_centerHorizontal="true"
                android:text="@string/message_str"
                android:textColor="@color/text_color_tab_style"
                android:textSize="10sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/usercenter_layout_indexActivity"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"


                >

                <ImageView
                    android:id="@+id/icon_usercenter_narmal_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:src="@drawable/icon_usercenter_click" />


                <ImageView
                    android:id="@+id/icon_look_me_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="-2dp"
                    android:layout_toRightOf="@+id/icon_usercenter_narmal_icon"
                    android:background="@drawable/bg_bubble"
                    android:gravity="center"
                    android:visibility="visible" />


            </RelativeLayout>


            <TextView
                android:id="@+id/icon_usercenter_narmal_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/icon_usercenter_narmal_icon"
                android:layout_centerHorizontal="true"
                android:text="@string/usercenter_str"
                android:textColor="@color/text_color_tab_style"
                android:textSize="10sp" />
        </LinearLayout>


    </LinearLayout>

</RelativeLayout>