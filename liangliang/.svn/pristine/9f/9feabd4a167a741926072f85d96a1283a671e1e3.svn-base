<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/text_color_eeeeee"
    android:orientation="vertical">



    <include layout="@layout/error_no_sign_up_layout" />
    <include layout="@layout/error_to_load_layout" />


    <LinearLayout
        android:id="@+id/sign_up_management_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <cn.chono.zxb.xrefreshview.XRefreshView
            android:id="@+id/sign_up_management_xrefreshview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:isHeightMatchParent="false"
            app:isWidthMatchParent="false">

            <android.support.v7.widget.RecyclerView
                android:id="@+id/sign_up_management_recyclerview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/text_color_eeeeee" />
        </cn.chono.zxb.xrefreshview.XRefreshView>


    </LinearLayout>


</FrameLayout>