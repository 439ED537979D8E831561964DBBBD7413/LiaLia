<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background"
        android:minHeight="8dp"
        android:maxHeight="8dp">
        <shape>
            <corners android:radius="8dp" />
            <gradient
                android:startColor="@color/text_color_d6e3e3"
                android:centerColor="@color/text_color_d6e3e3"
                android:endColor="@color/text_color_d6e3e3"
                android:centerY="0.45"
                android:angle="270"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient
                    android:angle="270.0"
                    android:centerColor="@color/text_color_ff7462"
                    android:centerY="0.75"
                    android:endColor="@color/text_color_ff7462"
                    android:startColor="@color/text_color_ff7462" />
            </shape>
        </clip>
    </item>
</layer-list>