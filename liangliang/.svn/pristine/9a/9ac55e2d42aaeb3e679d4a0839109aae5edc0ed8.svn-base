<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_light_gray_white"
    android:scrollbars="none" >

    <RelativeLayout
        android:id="@+id/appoint_detail_root_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:id="@+id/appoint_detail_scrollview_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/text_color_d3e1e1"
            android:layout_above="@+id/appoint_detail_bottom_layout"
            android:scrollbars="none"
            android:visibility="visible" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="54dp"
                    android:layout_marginTop="40px"
                    android:layout_gravity="center_horizontal"
                    >

                    <ImageView
                        android:id="@+id/appoint_detail_userImg_iv"
                        android:layout_width="54dp"
                        android:layout_height="54dp"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/message_head_default_icon" />

                    <ImageView
                        android:id="@+id/appoint_detail_level_men_iv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/appoint_detail_userImg_iv"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="-8dp"
                        android:layout_marginBottom="10dp"
                        android:src="@drawable/level_men_icon"
                        />

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="15dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/appoint_detail_name_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textSize="14sp"
                        android:text="yopper"
                        android:singleLine="true"
                        android:ellipsize="end"
                        android:maxWidth="140sp"
                        android:textColor="@color/text_color_423d46" />

                    <TextView
                        android:id="@+id/appoint_detail_sex_hor_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:drawablePadding="2dp"
                        android:paddingLeft="2dp"
                        android:paddingRight="2dp"
                        android:text="星座"
                        android:textColor="@color/text_color_white" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/appoint_detail_white_bg">


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/appoint_detail_datingtype_iv"
                            android:layout_width="17dp"
                            android:layout_height="17dp"
                            android:layout_marginLeft="16dp"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/chat_appoint_eat"/>

                        <TextView
                            android:id="@+id/appoint_detail_datingtype_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="15sp"
                            android:layout_marginLeft="8dp"
                            android:layout_gravity="center_vertical"
                            android:singleLine="true"
                            android:ellipsize="end"
                            android:textColor="@color/text_color_423f43"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/appoint_detail_costType_iv"
                                    android:layout_width="17dp"
                                    android:layout_height="16dp"
                                    android:layout_marginLeft="16dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/appoint_costtype_icon"/>

                                <TextView
                                    android:id="@+id/appoint_detail_costType_tv"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textSize="15sp"
                                    android:layout_marginLeft="8dp"
                                    android:layout_gravity="center_vertical"
                                    android:singleLine="true"
                                    android:ellipsize="end"
                                    android:textColor="@color/text_color_423f43"/>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="17dp"
                                    android:layout_height="16dp"
                                    app:layout_auto_baseheight="width"
                                    android:layout_marginLeft="16dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/appoint_item_time_icon"/>

                                <TextView
                                    android:id="@+id/appoint_detail_time_tv"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textSize="15sp"
                                    android:layout_marginLeft="8dp"
                                    android:layout_gravity="center_vertical"
                                    android:singleLine="true"
                                    android:ellipsize="end"
                                    android:textColor="@color/text_color_423f43"/>
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/appoint_detail_address_layout"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:layout_marginTop="10dp"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/appoint_detail_address_iv"
                                    android:layout_width="17dp"
                                    android:layout_height="16dp"
                                    app:layout_auto_baseheight="width"
                                    android:layout_marginLeft="16dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/appoint_item_address_icon"/>

                                <TextView
                                    android:id="@+id/appoint_detail_address_tv"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textSize="15sp"
                                    android:layout_marginLeft="8dp"
                                    android:layout_gravity="center_vertical"
                                    android:singleLine="true"
                                    android:ellipsize="end"
                                    android:textColor="@color/text_color_423f43"/>
                            </LinearLayout>



                        </LinearLayout>


                        <cn.chono.yopper.view.SquareHeightRelativeLayout
                            android:id="@+id/appoint_detail_dog_img_layout"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginRight="22dp">

                            <ImageView
                                android:id="@+id/appoint_detail_dog_img_iv"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:scaleType="fitXY"
                                />

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/image_shape_bg"
                                android:scaleType="fitXY"
                                />

                        </cn.chono.yopper.view.SquareHeightRelativeLayout>


                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="@color/text_color_b6bdc2"
                        android:layout_marginRight="12dp"
                        android:layout_marginLeft="12dp"
                        />

                    <RelativeLayout
                        android:id="@+id/appoint_detail_other_info_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >

                        <LinearLayout
                            android:id="@+id/appoint_detail_other_detail_layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:paddingBottom="10dp"
                            android:paddingTop="10dp"
                            android:layout_marginRight="12dp"
                            android:layout_marginLeft="12dp"
                            >

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/text_color_b6bdc2"
                            android:layout_marginRight="12dp"
                            android:layout_marginLeft="12dp"
                            android:layout_alignParentBottom="true"
                            />
                    </RelativeLayout>



                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="31dp"
                        android:paddingRight="12dp"
                        android:gravity="center_vertical|right">

                        <TextView
                            android:id="@+id/appoint_detail_look_num_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableLeft="@drawable/appoint_look_icon"
                            android:text="查看0"
                            android:gravity="center_vertical"
                            android:textSize="10sp"
                            android:drawablePadding="4dp"
                            android:textColor="@color/text_color_959492"
                            />
                        <TextView
                            android:id="@+id/appoint_detail_join_num_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableLeft="@drawable/appoint_join_icon"
                            android:text="已有0人报名"
                            android:layout_marginLeft="15dp"
                            android:gravity="center_vertical"
                            android:drawablePadding="4dp"
                            android:textSize="10sp"
                            android:textColor="@color/text_color_959492"
                            />

                    </LinearLayout>

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/appoint_detail_status_pb_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:orientation="vertical"
                    android:visibility="visible"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_marginLeft="8dp"
                        android:layout_height="37dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="手机"
                            android:textSize="14sp"
                            android:gravity="center"
                            android:textColor="@color/text_color_5a5664"/>

                        <TextView
                            android:id="@+id/appoint_detail_phone_status_tv"
                            android:layout_width="40dp"
                            android:layout_height="13dp"
                            android:text="已认证"
                            android:textSize="9sp"
                            android:layout_marginLeft="4dp"
                            android:layout_gravity="center_vertical"
                            android:gravity="center"
                            android:textColor="@color/text_color_white"
                            android:background="@drawable/appoint_phone_lable_shape"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="视频"
                            android:textSize="14sp"
                            android:gravity="center"
                            android:layout_marginLeft="26dp"
                            android:textColor="@color/text_color_5a5664"/>

                        <TextView
                            android:id="@+id/appoint_detail_video_status_tv"
                            android:layout_width="40dp"
                            android:layout_height="13dp"
                            android:text="未认证"
                            android:textSize="9sp"
                            android:layout_marginLeft="4dp"
                            android:layout_gravity="center_vertical"
                            android:gravity="center"
                            android:textColor="@color/text_color_5c6a6b"
                            android:background="@drawable/appoint_phone_no_lable_shape"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/appoint_detail_white_bg">

                        <LinearLayout
                            android:id="@+id/appoint_list_item_pb_layout"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="15dp"
                            android:layout_marginTop="15dp"
                            android:visibility="visible"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:layout_alignParentTop="true"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="56sp"
                                    android:layout_height="wrap_content"
                                    android:text="资料匹配"
                                    android:layout_marginLeft="14dp"
                                    android:gravity="center_vertical"
                                    android:textSize="14sp"
                                    android:textColor="@color/text_color_45424c"/>

                                <ProgressBar
                                    android:id="@+id/appoint_detail_info_pb"
                                    android:layout_width="120dp"
                                    android:layout_height="15dp"
                                    android:layout_marginLeft="8dp"
                                    android:max="100"
                                    style="?android:attr/progressBarStyleHorizontal"
                                    android:progressDrawable="@drawable/appoint_horizontal_pb_style"/>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:layout_marginTop="10dp"
                                android:layout_marginBottom="10dp"
                                android:layout_centerVertical="true"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="56sp"
                                    android:layout_height="wrap_content"
                                    android:text="安全指数"
                                    android:layout_marginLeft="14dp"
                                    android:gravity="center_vertical"
                                    android:textSize="14sp"
                                    android:textColor="@color/text_color_45424c"/>

                                <ProgressBar
                                    android:id="@+id/appoint_detail_safe_pb"
                                    android:layout_width="120dp"
                                    android:layout_height="15dp"
                                    android:layout_marginLeft="8dp"
                                    android:max="100"
                                    style="?android:attr/progressBarStyleHorizontal"
                                    android:progressDrawable="@drawable/appoint_horizontal_pb_style"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:layout_alignParentBottom="true"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="56sp"
                                    android:layout_height="wrap_content"
                                    android:text="亲密度"
                                    android:layout_marginLeft="14dp"
                                    android:gravity="center_vertical"
                                    android:textSize="14sp"
                                    android:textColor="@color/text_color_45424c"/>

                                <ProgressBar
                                    android:id="@+id/appoint_detail_qinmi_pb"
                                    android:layout_width="120dp"
                                    android:layout_height="15dp"
                                    android:layout_marginLeft="8dp"
                                    android:max="100"
                                    style="?android:attr/progressBarStyleHorizontal"
                                    android:progressDrawable="@drawable/appoint_horizontal_pb_style"/>
                            </LinearLayout>

                        </LinearLayout>

                        <cn.chono.yopper.view.ProgressBarView
                            android:id="@+id/appoint_detail_all_pbv"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_marginTop="15dp"
                            android:layout_marginBottom="15dp"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="12dp"
                            app:outside_round_width="2dp"
                            app:outside_round_color="#ffd3cc"
                            app:outside_round_progress_color="#ff735d"
                            app:inside_round_color="#ff735d"
                            app:inside_round_moment_from="4dp"
                            app:prompt_text_moment_from="3dp"
                            app:textColor="#ffffff"
                            app:promptTextColor="#ffffff"
                            app:promptTextSize="9sp"
                            app:textSize="13sp">
                        </cn.chono.yopper.view.ProgressBarView>

                    </LinearLayout>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="32dp"
                        android:layout_marginLeft="14dp"
                        android:textSize="10sp"
                        android:gravity="left|center_vertical"
                        android:text=" • 匹配度是对双方资料的综合评估,数值越高越适合约会"
                        android:textColor="@color/text_color_758888"/>
                </LinearLayout>

                <TextView
                    android:id="@+id/appoint_detail_taruser_hint_tv"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:text="已达成约会对象"
                    android:textSize="11sp"
                    android:textColor="@color/text_color_635d6b"
                    android:visibility="gone"
                    android:gravity="center_vertical"
                    />

                <RelativeLayout
                    android:id="@+id/appoint_detail_taruser_layout"
                    android:layout_width="match_parent"
                    android:layout_height="68dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginLeft="20dp"
                    android:visibility="gone"
                    android:background="@drawable/appoint_detail_white_bg"
                    >

                    <ImageView
                        android:id="@+id/appoint_detail_taruser_Img_iv"
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/message_head_default_icon" />

                    <TextView
                        android:id="@+id/appoint_detail_target_name_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/appoint_detail_taruser_Img_iv"
                        android:textSize="14sp"
                        android:text="name"
                        android:textColor="@color/text_color_423d46"
                        />

                    <TextView
                        android:id="@+id/appoint_detail_target_sex_hor_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="8dp"
                        android:layout_below="@+id/appoint_detail_target_name_tv"
                        android:layout_toRightOf="@+id/appoint_detail_taruser_Img_iv"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:drawablePadding="2dp"
                        android:paddingLeft="2dp"
                        android:paddingRight="2dp"
                        android:text="星座"
                        android:textColor="@color/text_color_white" />

                    <TextView
                        android:id="@+id/appoint_detail_target_time_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="9dp"
                        android:layout_marginRight="12dp"
                        android:textSize="11sp"
                        android:text="12-02"
                        android:textColor="@color/text_color_aeadab"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"/>

                    <TextView
                        android:id="@+id/appoint_detail_target_contact_tv"
                        android:layout_width="40dp"
                        android:layout_height="16dp"
                        android:layout_marginBottom="13dp"
                        android:layout_marginRight="12dp"
                        android:textSize="10sp"
                        android:text="联系ta"
                        android:gravity="center"
                        android:background="@drawable/appoint_detail_contact_bg"
                        android:textColor="@color/text_color_white"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"/>

                </RelativeLayout>
            </LinearLayout>

        </ScrollView>

        <LinearLayout
            android:id="@+id/appoint_detail_bottom_layout"
            android:layout_width="match_parent"
            android:layout_alignParentBottom="true"
            android:orientation="horizontal"
            android:background="@color/text_color_494850"
            android:layout_height="45dp">

            <TextView
                android:id="@+id/appoint_detail_bottom_left_tv"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text="先聊聊"
                android:textSize="16sp"
                android:gravity="center"
                android:textColor="@color/text_color_white"
                />

            <View
                android:id="@+id/appoint_detail_bottom_line"
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/text_color_b6bdc2"
                />

            <TextView
                android:id="@+id/appoint_detail_bottom_right_tv"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text="报名"
                android:textSize="16sp"
                android:gravity="center"
                android:textColor="@color/text_color_white"
                />

        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>