package cn.chono.yopper.data;

import java.io.Serializable;
import java.util.List;

public class Nearest implements Serializable {

	private int total;
	private int start;
	private int rows;

	private List<NearestList> list;

	public int getTotal() {
		return total;
	}

	public void setTotal(int total) {
		this.total = total;
	}

	public int getStart() {
		return start;
	}

	public void setStart(int start) {
		this.start = start;
	}

	public int getRows() {
		return rows;
	}

	public void setRows(int rows) {
		this.rows = rows;
	}

	public List<NearestList> getList() {
		return list;
	}

	public void setList(List<NearestList> list) {
		this.list = list;
	}

	@Override
	public String toString() {
		return "Nearest [total=" + total + ", start=" + start + ", rows="
				+ rows + ", list=" + list + "]";
	}

