		}

		public int getRank() {
			return rank;
		}

		public void setRank(int rank) {
			this.rank = rank;
		}

		public NearestReson getReason() {
			return reason;
		}

		public void setReason(NearestReson reason) {
			this.reason = reason;
		}

		public double getTotalScore() {
