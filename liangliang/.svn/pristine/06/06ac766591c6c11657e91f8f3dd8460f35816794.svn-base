<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!--兼容android 3.0以下的写法-->
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="#ffffff" />
            <corners android:radius="5dp"/>
        </shape>
    </item>

    <item android:right="5dp"
        >
        <shape
            android:shape="rectangle">
            <solid android:color="#ffffff" />
        </shape>
    </item>

</layer-list>
