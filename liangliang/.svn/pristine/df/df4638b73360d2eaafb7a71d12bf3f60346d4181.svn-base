<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="12px"
    android:paddingLeft="12px">

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/ffffff_lift_rectangle"
        android:orientation="vertical">

        <com.zhy.autolayout.AutoLinearLayout
            android:id="@+id/find_lift_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="visible"
            android:orientation="vertical">

            <com.zhy.autolayout.AutoLinearLayout

                android:id="@+id/find_lift_packet_title_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="14px">

                <View
                    android:layout_width="2px"
                    android:layout_height="match_parent"
                    android:background="@color/text_color_ff7462" />

                <TextView

                    android:id="@+id/find_lift_packet_title_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12px"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:text=""
                    android:textColor="@color/text_color_b2b2b2"
                    android:textSize="22px"

                    />


            </com.zhy.autolayout.AutoLinearLayout>


            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="20px"

                >

                <ImageView
                    android:id="@+id/find_lift_context_icon"
                    android:layout_width="88px"
                    android:layout_height="88px"
                    app:layout_auto_baseheight="width" />

                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12px"
                    android:orientation="vertical">

                    <com.zhy.autolayout.AutoLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView

                            android:id="@+id/find_lift_context_title_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:maxLines="1"


                            android:textColor="@color/text_color_333333"
                            android:textSize="30px"

                            />

                        <View
                            android:id="@+id/find_lift_prompt_view"
                            android:layout_width="10px"
                            android:layout_height="10px"
                            android:background="@drawable/bg_bubble"
                            android:gravity="center" />
                    </com.zhy.autolayout.AutoLinearLayout>


                    <TextView

                        android:id="@+id/find_lift_context_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12px"
                        android:textColor="@color/text_color_9a9a9a"
                        android:textSize="30px"

                        />

                </com.zhy.autolayout.AutoLinearLayout>


            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>

    </com.zhy.autolayout.AutoLinearLayout>


</com.zhy.autolayout.AutoLinearLayout>