package cn.chono.yopper.Service.Http.ReleaseAppointments;

import cn.chono.yopper.Service.Http.ParameterBean;
import cn.chono.yopper.data.MoviceDto;
import cn.chono.yopper.data.ShopDto;
import cn.chono.yopper.data.SportsDto;
import cn.chono.yopper.data.walkADogDto;

/**
 * Created by zxb on 2016/1/4.
 */
public class ReleaseAppointmentsBean extends ParameterBean {
    /**
     * 所在城市
     */
    private String city;
    private double lat;
    private double lng;
    /**
     * 消费类型
     */
    private int costType;
    private String endTime;
    /**
     * 约会时间，Unknown = 0, Morning = 1, Noon = 2, Afternoon = 3, Night = 4
     */
    private  int availableTime;

    private int sexRequired;

    private boolean videoRequired;
    /**
     * 活动类型
     */
    private int activityType;
    /**
     * 有商店的数据，没有不传
     */
    private ShopDto shop;
    /**
     * 电影的数据，没有不传
     */
    private MoviceDto movie;
    /**
     * 遛狗的数据，没有不传
     */
    private walkADogDto walkADog;
    /**
     * 运动的数据没有不传
     */
    private SportsDto sports;


    public String getCity() {
        return city;
    }

    public void setCity(String city) {
        this.city = city;
    }

    public double getLat() {
        return lat;
    }

    public void setLat(double lat) {
        this.lat = lat;
    }

    public double getLng() {
        return lng;
    }

    public void setLng(double lng) {
        this.lng = lng;
    }

    public int getCostType() {
        return costType;
    }

    public void setCostType(int costType) {
        this.costType = costType;
    }

    public String getEndTime() {
        return endTime;
    }

    public void setEndTime(String endTime) {
        this.endTime = endTime;
    }

    public int getAvailableTime() {
        return availableTime;
    }

    public void setAvailableTime(int availableTime) {
        this.availableTime = availableTime;
    }

    public int getSexRequired() {
        return sexRequired;
    }

    public void setSexRequired(int sexRequired) {
        this.sexRequired = sexRequired;
    }

    public boolean isVideoRequired() {
        return videoRequired;
    }

    public void setVideoRequired(boolean videoRequired) {
        this.videoRequired = videoRequired;
    }

    public int getActivityType() {
        return activityType;
    }

    public void setActivityType(int activityType) {
        this.activityType = activityType;
    }

    public ShopDto getShop() {
        return shop;
    }

    public void setShop(ShopDto shop) {
        this.shop = shop;
    }

    public MoviceDto getMovie() {
        return movie;
    }

    public void setMovie(MoviceDto movie) {
        this.movie = movie;
    }

    public walkADogDto getWalkADog() {
        return walkADog;
    }

    public void setWalkADog(walkADogDto walkADog) {
        this.walkADog = walkADog;
    }

    public SportsDto getSports() {
        return sports;
    }

    public void setSports(SportsDto sports) {
        this.sports = sports;
    }
}
