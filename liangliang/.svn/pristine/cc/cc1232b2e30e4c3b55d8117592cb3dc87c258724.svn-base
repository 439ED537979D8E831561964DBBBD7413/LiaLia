<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <corners android:radius="20dp" />
    <!-- 圆角半径 -->

    <stroke
        android:width="1dp"
        android:color="#818fb7" />
    <!-- 矩形的边线 -->

    <padding
        android:bottom="2dp"
        android:left="10dp"
        android:right="10dp"
        android:top="2dp" />

    <gradient
        android:endColor="#cbd6d8"
        android:startColor="#cbd6d8" />
    <!-- 矩形的内部颜色，这里设置成白色，即空心 -->



</shape>