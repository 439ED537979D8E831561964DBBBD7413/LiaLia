<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/corners_white_default"
    android:gravity="center_vertical"
    android:paddingTop="12dp"
    android:paddingBottom="12dp"
    android:paddingLeft="8dp"
    android:paddingRight="8dp"
    android:orientation="horizontal">


    <ImageView
        android:id="@+id/sign_up_management_group_target_user_icon_iv"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginRight="12dp" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/corners_white_default"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/sign_up_management_group_target_user_name_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:maxEms="7"
                android:singleLine="true"
                android:textColor="@color/text_color_333333"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/sign_up_management_group_target_user_constellation_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/dating_male_bg"
                android:gravity="center_vertical"
                android:singleLine="true"
                android:textColor="@color/text_color_white"
                android:textSize="10sp" />
        </LinearLayout>

        <TextView


            android:id="@+id/sign_up_management_group_target_user_similarity_tv"
            android:layout_width="wrap_content"
            android:layout_height="0dp"

            android:layout_marginTop="10dp"
            android:layout_weight="1"

            android:ellipsize="end"
            android:gravity="center_vertical"
            android:maxLines="1"
            android:textColor="@color/text_color_ff7462"
            android:textSize="14sp" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:orientation="vertical">


        <TextView
            android:id="@+id/sign_up_management_group_target_time_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_gravity="right"
            android:ellipsize="end"
            android:gravity="center_vertical"
            android:maxLines="1"
            android:text="联系Ta"
            android:textColor="@color/text_color_b2b2b2"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/sign_up_management_group_target_contact_but"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginTop="12dp"
            android:background="@drawable/corners_orange_selected_style"
            android:ems="4"
            android:gravity="center"
            android:maxLines="1"
            android:paddingBottom="3dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingTop="3dp"
            android:text="联系Ta"
            android:textColor="@drawable/font_selected_style_ffffff"
            android:textSize="10sp" />


    </LinearLayout>


</LinearLayout>