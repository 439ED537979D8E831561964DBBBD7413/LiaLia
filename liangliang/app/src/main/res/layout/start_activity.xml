<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/start_root_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_launch"
    android:fitsSystemWindows="true"
    android:orientation="vertical">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/start_launch_version_tv"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="8px"
        android:src="@drawable/ic_start_lanunch" />


    <TextView
        android:id="@+id/start_launch_version_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="14dp"
        android:text=""
        android:textColor="@color/color_e7a3ad"
        android:textSize="12sp"
        android:visibility="invisible" />


</RelativeLayout>