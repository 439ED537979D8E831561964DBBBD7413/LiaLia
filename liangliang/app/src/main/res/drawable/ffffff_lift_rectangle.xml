<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!--兼容android 3.0以下的写法-->
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="#ffffff" />
            <corners android:radius="5dp"/>

            <!-- 外框，框線粗細，框線顏色 -->
            <stroke
                android:width="3dip"
                android:color="#ffffff"
                />
        </shape>
    </item>

    <item
        android:left="5dp"
        >
        <shape
            android:shape="rectangle">
            <solid android:color="#ffffff" />

            <!-- 外框，框線粗細，框線顏色 -->
            <stroke
                android:width="2dip"
                android:color="#ffffff"
                />
        </shape>
    </item>

</layer-list>
