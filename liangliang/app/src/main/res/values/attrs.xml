        <attr name="kswThumbMarginTop" format="dimension|reference"/>

        <attr name="kswThumbMarginBottom" format="dimension|reference"/>

        <attr name="kswThumbMarginLeft" format="dimension|reference"/>

        <attr name="kswThumbMarginRight" format="dimension|reference"/>

        <attr name="kswThumbWidth" format="dimension|reference"/>

        <attr name="kswThumbHeight" format="dimension|reference"/>

        <attr name="kswThumbRadius" format="dimension|reference"/>

        <attr name="kswBackRadius" format="dimension|reference"/>

        <attr name="kswBackDrawable" format="reference"/>

        <attr name="kswBackColor" format="color|reference"/>

        <attr name="kswFadeBack" format="boolean"/>

        <attr name="kswBackMeasureRatio" format="float"/>

        <attr name="kswAnimationDuration" format="integer"/>

        <attr name="kswTintColor" format="color|reference"/>

    </declare-styleable>


    <declare-styleable name="ProgressBarView">

        <!-- 进度条的背景色 -->
        <attr name="outside_round_color" format="color"/>

        <!-- 进度条View的大小 -->
        <attr name="outside_round_width" format="dimension"/>

        <!-- 进度条的%进度的颜色-->
        <attr name="outside_round_progress_color" format="color"/>

        <!-- 进度条的样式，0表示空心。1表示实心 -->
        <attr name="outside_round_progress_style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>

        <!-- 进度条背景圆环的样式，0表示空心。1表示实心 -->
        <attr name="outside_round_style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
        <!-- 中间显示百分比%的颜色 -->
        <attr name="textColor" format="color"/>
        <!-- 中间显示百分比%的字体大小 -->
        <attr name="textSize" format="dimension"/>
        <!-- 中间显示百分比% 显否显示-->
        <attr name="textIsDisplayable" format="boolean"></attr>

        <!-- 中间提示方案的颜色 -->
        <attr name="promptTextColor" format="color"/>
        <!-- 中间提示方案的大小 -->
        <attr name="promptTextSize" format="dimension"/>
        <!-- 中间提示方案的是否显示 -->
        <attr name="promptTextIsDisplayable" format="boolean"></attr>

        <!-- 进度条内框与外框的间矩  -->
        <attr name="inside_round_moment_from" format="dimension"/>

        <!--  进度条内框与外框有间矩的背景色 -->
        <attr name="inside_round_color" format="color"/>
        <!-- 百分比与提示文字 的矩离 -->
        <attr name="prompt_text_moment_from" format="dimension"/>



    </declare-styleable>
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension"/>
